@model List<MHR_EF_Code.Models.Entities.department>

<table class="table">
    <tr>
        <th>id</th>
        <th>Name</th>
        <th>location</th>
    </tr>
    @foreach (var item in Model)
    {
     <tr>
          <td>@item.DepartmentID</td>
          <td>@item.Name</td>
          <td>@item.location</td>
    </tr>
    }
</table>